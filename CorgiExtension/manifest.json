{
	"name": "GiveCorgi",
	"description": "Gives an image of a corgi with the press of a button",
	"version": "1.0",

	"manifest_version": 2,
	"browser_action":
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html" 
	},
	"permissions":
	[
		"<all_urls>",
		"tabs",
		"bookmarks"

	]

}